<h2>People</h2>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <% @people.each do |p|%>
      <tr>
        <td><%= p.first_name %></td>
        <td><%= p.last_name %></td>
        <td><%= link_to 'Edit', edit_person_path(p.id) %></td>
        <td><%= link_to 'Destroy', person_path(p.id), method: "delete" %></td>
      </tr>
  <% end %>
</table>